CREATE TABLE `m_params_options` (  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,  `param_id` int(10) unsigned NOT NULL,  `title` int(10) unsigned NOT NULL COMMENT 'dstring',  `order` int(10) unsigned NOT NULL,  `active` tinyint(1) unsigned NOT NULL DEFAULT '0',  PRIMARY KEY (`id`),  KEY `param_id` (`param_id`),  CONSTRAINT `m_params_id` FOREIGN KEY (`param_id`) REFERENCES `m_params` (`id`) ON DELETE CASCADE ON UPDATE CASCADE) ENGINE=InnoDB DEFAULT CHARSET=utf8